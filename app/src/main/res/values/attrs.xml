<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PagosGallery">
        <attr name="android:galleryItemBackground" />
    </declare-styleable>

    <declare-styleable name="CustomFontTextView">

        <attr name="font_type" format="enum">
            <enum name="angelina" value="0" />
            <enum name="glyphicons_halflings_regular" value="1" />
            <enum name="SourceSansPro_Light" value="2" />
            <enum name="SourceSansPro_Regular" value="3" />
            <enum name="SourceSansPro_Semibold" value="4" />
            <enum name="Bupkis" value="5" />
        </attr>
    </declare-styleable>

    <declare-styleable name="MaterialImageButton">
        <attr name="imageSrc" format="reference" />
        <attr name="buttonRippleColor"/>
        <attr name="imageTint" format="color" />
        <attr name="imagePadding" format="dimension" />
        <attr name="imageStartPadding" format="dimension" />
        <attr name="imageEndPadding" format="dimension" />
        <attr name="imageTopPadding" format="dimension" />
        <attr name="imageBottomPadding" format="dimension" />
    </declare-styleable>

    <declare-styleable name="MaterialButton">
        <attr name="intercepterOnclick" format="boolean" />
        <attr name="buttonRippleColor2" format="color" />
        <attr name="buttonNormalColor" format="color" />
        <attr name="selectedTextColor" format="color" />
        <attr name="cornerRadius"/>
        <attr name="buttonStrokeColor"/>
        <attr name="overBackground" format="reference" />
        <attr name="buttonShape" />

        <attr name="filling_color" format="color" />
        <attr name="outline_color" format="color" />

    </declare-styleable>
    <declare-styleable name="StyleButton">
        <attr name="protectOnclick" format="boolean" />
    </declare-styleable>

    <declare-styleable name="MaterialLinearLayout">
        <attr name="buttonRippleColor"/>
        <attr name="cornerRadius"/>
        <attr name="buttonStrokeColor"/>
        <attr name="selectedStrokeColor"/>
        <attr name="normalBackGround" format="reference"/>
    </declare-styleable>

    <!-- comunes -->
    <attr name="buttonRippleColor" format="color"/>
    <attr name="cornerRadius" format="dimension"/>
    <attr name="buttonStrokeColor" format="color"/>
    <attr name="selectedStrokeColor" format="color"/>


    <declare-styleable name="StyleTextView">
        <attr name="tipo" format="enum">
            <enum name="Titulo" value="0" />
            <enum name="Subtitulo" value="1" />
            <enum name="Descripcion" value="2" />
            <enum name="Indicacion" value="3" />
            <enum name="Text" value="4" />
            <enum name="InputText" value="4" />
            <enum name="InputTextDescription" value="4" />
        </attr>
    </declare-styleable>

    <declare-styleable name="CustomSeekBar">
        <attr name="text" format="string"/>
        <attr name="textSize" format="dimension"/>
    </declare-styleable>

    <attr name="buttonShape" format="enum">
        <enum name="rectangle" value="0" />
        <enum name="oval" value="1" />
    </attr>

    <declare-styleable name="ValidatedEdittext">
        <attr name="tipovalidacion" format="enum">
            <enum name="email" value="0"/>
            <enum name="password" value="1"/>
            <enum name="phone" value="2"/>
            <enum name="zipcode" value="3"/>
            <enum name="text" value="4"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="CustomValidationEditText">
        <attr name="formato" format="enum">
            <enum name="email" value="0"/>
            <enum name="password" value="1"/>
            <enum name="phone" value="2"/>
            <enum name="zipcode" value="3"/>
            <enum name="text" value="4"/>
            <enum name="number" value="5"/>
            <enum name="celphone" value="6"/>
        </attr>
        <attr name="hintText" format="string"/>
        <attr name="maxLength" format="integer"/>
        <attr name="imageIsGone" format="boolean"/>
        <attr name="maxLines" format="integer"/>
        <attr name="isSingleLine" format="boolean"/>
        <attr name="isTextEnabled" format="boolean"/>
        <attr name="android:inputType"/>
    </declare-styleable>

    <declare-styleable name="BorderTitleLayout">
        <attr name="title" format="string"/>
    </declare-styleable>

    <declare-styleable name="GenericTabLayout">
        <attr name="tabLayout" format="reference"/>
    </declare-styleable>

    <declare-styleable name="SeekBarNoTouch">
        <attr name="seekThumbColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="MontoTextView">
        <attr name="dotBottom" format="boolean"/>
        <attr name="symbolSmaller" format="boolean"/>
        <attr name="decimalLenght" format="integer"/>
    </declare-styleable>

</resources>