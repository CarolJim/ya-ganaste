<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".modules.usernovalid.ProspectoUserFragment">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            >
            <include
                android:id="@+id/toolbarLy"
                layout="@layout/toolbar"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <com.pagatodo.yaganaste.utils.customviews.StyleTextView
                android:id="@+id/titulo_datos_valid"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="8dp"
                android:text="Estamos revisando tu información"
                android:textAlignment="center"
                android:textColor="@color/colorAccent"
                android:textSize="18sp"
                app:layout_constraintTop_toTopOf="parent"
                app:tipo="Text"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="153dp" />


            <com.pagatodo.yaganaste.utils.customviews.StyleTextView
                android:id="@+id/subtitulo_datos_valid"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="8dp"
                android:text="Estamos revisando tu información"
                android:textAlignment="center"
                android:textColor="@color/gray_text_wallet_3"
                android:textSize="18sp"
                app:layout_constraintTop_toTopOf="parent"
                app:tipo="Text"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="153dp" />

        </LinearLayout>

</FrameLayout>