<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".modules.sidebar.DataAccount.AccountDataFragment">

    <LinearLayout
        android:id="@+id/linearLayout7"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="vertical"
        app:layout_constraintTop_toTopOf="parent">

        <com.pagatodo.yaganaste.utils.customviews.StyleTextView
            android:id="@+id/title"
            style="@style/generic_text_style_2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="32dp"
            android:gravity="center"
            android:text="@string/navigation_drawer_mydata"
            android:textColor="@color/colorAccent" />

        <ImageView
            android:id="@+id/image_user"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/title_menu"
            tools:ignore="MissingConstraints" />

        <com.pagatodo.yaganaste.utils.customviews.StyleTextView
            android:id="@+id/subtitle"
            style="@style/generic_text_style_5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="32dp"
            android:gravity="center"
            android:text="@string/change_photo"
            android:textColor="@color/gray_slate" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/showName"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="25dp"
        android:orientation="vertical"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout7"
        tools:ignore="MissingConstraints">

        <com.pagatodo.yaganaste.utils.customviews.StyleTextView
            android:id="@+id/display_name"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="0dp"
            android:text="@string/nombre_simple"
            android:textColor="@color/colorAccent" />

        <com.pagatodo.yaganaste.utils.customviews.StyleTextView
            android:id="@+id/display_names"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="5dp"
            android:text="Cecilia Romero"
            android:textColor="@color/gray_slate"
            android:textSize="16sp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/space_div1"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="4dp"
        android:layout_marginRight="30dp"
        android:background="#e0e0e0"
        android:orientation="horizontal"
        app:layout_constraintTop_toBottomOf="@+id/showName"></LinearLayout>

    <LinearLayout
        android:layout_marginTop="10dp"
        android:id="@+id/showEmail"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintTop_toBottomOf="@+id/space_div1"
        tools:ignore="MissingConstraints">

        <com.pagatodo.yaganaste.utils.customviews.StyleTextView
            android:id="@+id/display_email"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="0dp"
            android:text="@string/correo_electronico"
            android:textColor="@color/colorAccent" />

        <com.pagatodo.yaganaste.utils.customviews.StyleTextView
            android:id="@+id/display_emails"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="5dp"
            android:text="ceciliaromero@correo.com"
            android:textColor="@color/gray_slate"
            android:textSize="16sp" />
    </LinearLayout>
    <LinearLayout
        android:id="@+id/space_div2"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="10dp"
        android:layout_marginRight="30dp"
        android:background="#e0e0e0"
        android:orientation="horizontal"
        app:layout_constraintTop_toBottomOf="@+id/showEmail"></LinearLayout>

    <LinearLayout
        android:layout_marginTop="10dp"
        android:id="@+id/showPhone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintTop_toBottomOf="@+id/space_div2"
        tools:ignore="MissingConstraints">

        <com.pagatodo.yaganaste.utils.customviews.StyleTextView
            android:id="@+id/display_phone"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="0dp"
            android:text="@string/telefono_cupo_hint"
            android:textColor="@color/colorAccent" />

        <com.pagatodo.yaganaste.utils.customviews.StyleTextView
            android:id="@+id/display_phones"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="5dp"
            android:text="55 5555 5555"
            android:textColor="@color/gray_slate"
            android:textSize="16sp" />
    </LinearLayout>
    <LinearLayout
        android:id="@+id/space_div3"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="10dp"
        android:layout_marginRight="30dp"
        android:background="#e0e0e0"
        android:orientation="horizontal"
        app:layout_constraintTop_toBottomOf="@+id/showPhone"></LinearLayout>

    <!--<LinearLayout
        android:layout_marginTop="10dp"
        android:id="@+id/showDate"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
